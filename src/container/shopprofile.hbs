<main class="p-5">
    <div class="row">
        <div class="col-12 ">
            <div class="card">
                <div class="card-body">
                    <div class="nav nav tabs">
                        {{#with navigation}}
                        {{#each links}}
                        <div class="nav-item">
                            <a href="{{href}}" class="nav-link {{#if @last}}disabled{{/if}}">{{text}}</a>
                        </div>
                        {{/each}}
                        {{/with}}
                    </div>
                </div>
            </div>

        </div>
        <div class="mb-5">
            <h2 class="text-center text-header text-bg mb-5">{{header}}</h2>
        </div>
        <section class="mb-3">
            {{#with purchase}}
            <div class="col-12">
                <div class="nav nb-6">
                    {{#each tabs}}
                    <div class="nav nav-tabs my-2">
                        <div class="nav-item ">
                            <div class="nav-link {{#if @last class=" text-primary"}}primary{{/if}} {{#if @first
                                class="text-secondary" }}active {{/if}}">{{tabs}}{{this}}</div>
                        </div>
                    </div>
                    {{/each}}
                </div>
                <div class="mb-5">
                    <hr>
                </div>

                <section class="col-12">
                    <div class-="row">

                        <div class="col-12 ps-2 ms-5 ">
                            {{#with actionBlock}}

                            {{#with input}}
                            <input type="text" class="form-control-sm" placeholder="Placeholder" aria-label="Username"
                                aria-describedby="basic-addon1">

                            {{/with}}
                            {{#each buttons}}
                            {{#if isSuccess includeZero=true }}<a href=" {{link}}" class=" btn btn-success">{{text}}</a>
                            {{/if}}
                            {{#if isOutline includeZero=true}}
                            <a href="{{link}}" class=" btn btn-success-outline">
                                {{text}}</a>{{/if}}
                            {{#if isDanger includeZero=true}}
                            <a href="{{link}}" class=" btn btn-danger">{{text}}</a>{{/if}}
                            {{/each}}

                            {{/with}}

                            <hr class="m-5">

                            <section class="col-12">
                                <div class="col-12">
                                    <div class="table table-striped ">
                                        <div class="row">
                                            {{#each itemList}}
                                            <div class="row">
                                                <div class="col-1 my-3 ms-2 pt-3">
                                                    <p class="text-sm">{{number}}</p>
                                                    <h6>{{status}}</h6>
                                                </div>
                                                <div class="col-1 ms-3 my-5 pt-1">
                                                    {{#with amount}}
                                                    <p>{{title}}</p>
                                                    <p>{{value}}</p>
                                                    {{/with}}
                                                </div>
                                                {{#each images}}
                                                <div class="card  col-2 mx-1 g-1">
                                                    <img class='card-img' src={{this}}>
                                                </div>
                                                {{/each}}
                                                <div class="col-3 my-2 ">
                                                    {{#with button}}
                                                    {{#if isPrimary includeZero=true}}
                                                    <a href="{{link}}"
                                                        class="text-light btn btn-primary">{{text}}</a>{{/if}}
                                                    {{/with}}
                                                </div>

                                            </div>

                                            {{/each}}
                                        </div>

                                    </div>
                                </div>
                        </div>
                </section>
            </div>
            {{/with}}
        </section>

        <div clas="col-12">
            <hr>
        </div>
        <section class="col-12">
            <div>
                <div class="card">
                    {{#with userInfo}}
                    <h2 class="text-center title text-dark">{{title}}</h2>
                    <div class="col-12">
                        <div class="card">

                            {{#with profileData}}
                            <h5 class="text-center text-dark title">{{title}}</h5>
                            <div class="col-12 g-2 my-2">
                                <div class="card text-center text-dark title">
                                    {{#with fullName}}
                                    <div class="row">
                                        <div class="col-4 my-2">
                                            {{#with surname}}
                                            <p>{{title}}</p>
                                            <p>{{value}}</p>
                                            {{/with}}
                                        </div>

                                        <div class="col-4 my-2">
                                            {{#with name}}
                                            <p>{{title}}</p>
                                            <p>{{value}}</p>
                                            {{/with}}
                                        </div>
                                        <div class="col-4 my-2">
                                            {{#with middleName}}
                                            <p>{{title}}</p>
                                            <p>{{value}}</p>
                                            {{/with}}
                                        </div>
                                    </div>


                                    {{/with}}
                                </div>
                            </div>


                            <div clas='col-12 my-2 mb-2'>
                                <div class="card text-center text-dark title mb-2">
                                    {{#with otherInfo}}
                                    <div class="row">
                                        <div class="col-4 my-2">
                                            {{#with birthday}}
                                            <p>{{title}}</p>
                                            <p>{{value}}</p>
                                            {{/with}}
                                        </div>
                                        <div class="col-4 my-2">
                                            {{#with sex}}
                                            <p>{{title}}</p>
                                            <p>{{value}}</p>
                                            {{/with}}
                                        </div>
                                        <div class="col-4 my-2">
                                            {{#with language}}
                                            <p>{{title}}</p>
                                            <p>{{value}}</p>
                                            {{/with}}
                                        </div>
                                    </div>
                                    {{/with}}
                                </div>
                            </div>


                            {{/with}}
                            <div class="col-12  mb-2 ">
                                <div class="card mb-2">
                                    {{#each buttons}}
                                    <div class="row">
                                        <div class="col-3 mx-5 ">
                                            {{#if isPrimary includeZero=true}}
                                            <a href="{{link}}" class="text-light btn btn-primary">{{text}}</a>
                                            {{/if}}
                                        </div>
                                        <div class="col-3  ">
                                            {{#if isSecondary includeZero=true}}
                                            <a href="{{link}}" class="text-light btn btn-secondary">{{text}}</a>
                                            {{/if}}
                                        </div>
                                        <div class="col-3 pt-1 ">
                                            {{#if isDanger includeZero=true}}
                                            <a href="{{link}}" class="text-light btn btn-danger">{{text}}</a>
                                            {{/if}}
                                        </div>
                                    </div>
                                    {{/each}}
                                </div>
                            </div>
                            <section class="col-12">
                                <div class="col-12 mb-2">
                                    <div class="card text-center text-dark title">
                                        {{#with recipients}}
                                        <h2 class="text-center title text-dark">{{title}}</h2>
                                        <div class="col-12 mx-2">
                                            <div class="row">
                                                {{#each users}}
                                                <div class="col-4 my-3 pt-2 mx-5">
                                                    <div class="row">
                                                        <div class="card">
                                                            <h6 class="title text-center-primary">{{name}}</h6>
                                                            <p>{{phone}}</p>
                                                            <div class="col-2 mx-5 my-2">
                                                                {{#with button}}
                                                                <a href="link" class="btn btn-primary">{{text}}</a>
                                                                {{/with}}
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                {{/each}}
                                            </div>
                                        </div>
                                        {{/with}}
                                    </div>
                                </div>
                            </section>
                            <section class="col-12 ">
                                <div class="col-12 ">
                                    <div class="card text-center text-dark title">
                                        {{#with contacts}}
                                        <h2 class="text-center title text-dark">{{title}}</h2>
                                        <div class="col-12">
                                            <div class="row">
                                                {{#each contactData}}
                                                <div class="col-3 mb-2 mx-1">
                                                    <div class="card">
                                                        <div class="row">
                                                            <h6>{{title}}</h6>
                                                            <p>{{value}}</p>
                                                            {{#with button}}
                                                            <div class="col-3 ms-1 ps-5 mb-1">
                                                                {{#if isPrimary includeZero=true}}
                                                                <a href="link"
                                                                    class="text-light btn btn-primary p-2">{{text}}</a>
                                                                {{/if}}
                                                            </div>
                                                            <div class="col-2 mb-1">
                                                                {{#if isSecondary includeZero=true}}
                                                                <a href="link"
                                                                    class="text-light btn btn-secondary p-3">{{text}}</a>
                                                                {{/if}}
                                                            </div>
                                                            <div class="col-2 mb-1 mx-0 ">
                                                                {{#if isDanger includeZero=true}}
                                                                <a href="link"
                                                                    class="text-light btn btn-danger p-3">{{text}}</a>
                                                                {{/if}}
                                                            </div>

                                                            {{/with}}
                                                        </div>
                                                    </div>

                                                </div>
                                                {{/each}}

                                            </div>
                                        </div>
                                        {{/with}}
                                    </div>
                                </div>
                            </section>
                        </div>
                    </div>
                    {{/with}}
                </div>
            </div>
        </section>




        <hr class="mb-5">


        <hr class="mb-5">

        <section class=" col-12 mb-5">
            <div class="nav">
                <div class="row">
                    {{#each footer}}
                    <div class="col-12">
                        <div class="row">
                            {{#each this }}
                            <div class="col-2">
                                <div class="row">
                                    <div class="nav flex-column">
                                        <div class="nav-item mx-5 mb-2">
                                            <a href="{{link}}"
                                                class="nav-link  {{#if @last}}disabled{{/if}}">{{text}}</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
        </section>

    </div>
</main>